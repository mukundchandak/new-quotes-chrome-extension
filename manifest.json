{
    "manifest_version": 2,
    "name": "Wordy++ext",
    "description": "Famous Quotes",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "chrome_url_overrides": {
        "newtab": "popup.html"
    }
}